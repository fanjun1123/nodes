#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_egg-ts-helper@1.20.0@egg-ts-helper/dist/bin.js" "$@"
  ret=$?
else 
  node  "$basedir/../_egg-ts-helper@1.20.0@egg-ts-helper/dist/bin.js" "$@"
  ret=$?
fi
exit $ret
